<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #d1e3dd;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.project12.AddRestaurantController">
    <children>
        <AnchorPane prefHeight="118.0" prefWidth="600.0" style="-fx-background-color: #02111b;">
            <children>
                <Text fx:id="header" fill="#d1e3dd" layoutX="222.0" layoutY="82.0" strokeType="OUTSIDE" strokeWidth="0.0" text="CHEFx" textAlignment="CENTER">
                    <font>
                        <Font name="Verdana Bold Italic" size="44.0" />
                    </font>
                </Text>
                <Button fx:id="backinAddRestaurant" layoutX="21.0" layoutY="16.0" mnemonicParsing="false" onAction="#onBackActionInAddRestaurant" style="-fx-background-color: #d1e3dd;" text="Log Out">
